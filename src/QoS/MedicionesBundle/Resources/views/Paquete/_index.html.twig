{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Paquete list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tam</th>
                <th>Unidadtam</th>
                <th>Url</th>
                <th>Fullurl</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('Paquete_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.tam }}</td>
                <td>{{ entity.unidadTam }}</td>
                <td>{{ entity.url }}</td>
                <td>{{ entity.fullUrl }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('Paquete_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('Paquete_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('Paquete_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
